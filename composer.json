{
    "name": "appstract/twikey-php-client",
    "description": "PHP Client for the Twikey API",
    "keywords": [
        "appstract",
        "pakketpartner-php-client"
    ],
    "homepage": "https://github.com/appstract/twikey-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Olav van Schie",
            "email": "olav@appstract.nl",
            "homepage": "https://appstract.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "jenssegers/model": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Appstract\\Pakketpartner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appstract\\Pakketpartner\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
